<?xml version="1.0"?>

<PropertyList>

 <sim>
  <description>F-16 JSBSim</description>
  <author>Minh Le</author>
  <status>production</status>
  <rating>
    <FDM type="int">3</FDM>
    <systems type="int">4</systems>
    <cockpit type="int">4</cockpit>
    <model type="int">4</model>
  </rating>
  <flight-model>jsb</flight-model>
  <aero>RC-f16</aero>
